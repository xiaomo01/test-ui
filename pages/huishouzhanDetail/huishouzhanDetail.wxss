.info_main{
  border-bottom: 1px solid #eeeeee;
}
.info_content{
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  display: -moz-box;
  -moz-line-clamp: 2;
  -moz-box-orient: vertical;
  word-wrap: break-word;
  word-break: break-all;
  white-space: normal;    
}
.score_img{
  height: 40rpx;
  width: 40rpx;
  display: inline-block
}